<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="439.0" prefWidth="559.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demojavafx.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button layoutX="280.0" layoutY="14.0" onAction="#upButton" prefHeight="40.0" prefWidth="135.0" text="Up" textAlignment="CENTER" textFill="#090a0ad6">
      <font>
         <Font name="Fira Code Retina" size="15.0" />
      </font></Button>
   <Button layoutX="528.0" layoutY="200.0" onAction="#rightButton" prefHeight="40.0" prefWidth="140.0" text="Right" textAlignment="CENTER" textFill="#090a0ad6">
      <font>
         <Font name="Fira Code Retina" size="15.0" />
      </font>
   </Button>
   <Button layoutX="266.0" layoutY="396.0" onAction="#downButton" prefHeight="39.0" prefWidth="135.0" text="Down" textAlignment="CENTER" textFill="#090a0ad6">
      <font>
         <Font name="Fira Code Retina" size="15.0" />
      </font>
   </Button>
   <Button alignment="CENTER" layoutX="14.0" layoutY="200.0" onAction="#leftButton" prefHeight="41.0" prefWidth="134.0" text="Left" textAlignment="CENTER" textFill="#090a0ad6">
      <font>
         <Font name="Fira Code Retina" size="15.0" />
      </font>
   </Button>
   <Circle fx:id="actionCircle" fill="DODGERBLUE" layoutX="334.0" layoutY="221.0" radius="47.0" stroke="BLACK" strokeType="INSIDE" />
   <Button fx:id="exitButton" layoutX="97.0" layoutY="486.0" mnemonicParsing="false" onAction="#exitButton" prefHeight="35.0" prefWidth="73.0" text="Exit" textAlignment="CENTER">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
   <Button layoutX="480.0" layoutY="486.0" mnemonicParsing="false" onAction="#switchToAboutScene" prefHeight="35.0" prefWidth="156.0" text="About this program">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
   <Button fx:id="exitButton1" layoutX="231.0" layoutY="487.0" mnemonicParsing="false" onAction="#fullScreenButton" prefHeight="34.0" prefWidth="206.0" text="FullScreen enable" textAlignment="CENTER">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
</AnchorPane>
